<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mrye.propertyanimation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CustomAnimation.ListViewTween"
            android:label="Custom Animation"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".ViewAnimation"
            android:label="View Animation"
            android:theme="@style/AppTheme" />
        <activity android:name=".LayoutTransition" />
        <activity android:name=".SVGActivity" />
        <activity android:name=".PAnimatorAcitvity"
            android:label="Property Animation"
            android:theme="@style/AppTheme"/>
    </application>

</manifest>